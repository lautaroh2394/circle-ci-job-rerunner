{
    "manifest_version": 3,
    "version": "0.1",
    "name": "CircleCI Job Rerun",
    "permissions": [
        "storage"
    ],
    "options_ui": {
        "page": "options.html"
    },
    "content_scripts": [
        {
          "matches": ["https://app.circleci.com/*/*/workflows/*/jobs/*"],
          "js": ["content-script.js"],
          "runAt": "document_end"
        }
    ]
}